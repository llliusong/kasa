<?xml version="1.0" encoding="UTF-8" ?><configuration scan="true" scanPeriod="1 seconds">    <!--<include resource="org/springframework/boot/logging/logback/base.xml"/>-->    <property name="LOG_HOME" value="${logback.path}"/>    <!-- 彩色日志格式 -->    <property name="CONSOLE_LOG_PATTERN"              value="%black(控制台-) %red(%d{yyyy-MM-dd HH:mm:ss}) %green([%thread]) %highlight(%-5level) %boldMagenta(%logger{64}) - %cyan(%msg%n)"/>    <!-- Console 输出设置 -->    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">        <encoder>            <pattern>${CONSOLE_LOG_PATTERN}</pattern>            <charset>utf8</charset>        </encoder>    </appender>    <appender name="CATALINA" class="ch.qos.logback.core.FileAppender">        <File>${LOG_HOME}/catalina.log</File>        <encoder>            <pattern>${CONSOLE_LOG_PATTERN}</pattern>            <charset>utf8</charset>        </encoder>    </appender>    <appender name="INFO_FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">        <File>${LOG_HOME}/community-info.log</File>        <rollingPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedRollingPolicy">            <FileNamePattern>${LOG_HOME}/community-info.%d{yyyy-MM-dd}.%i.log</FileNamePattern>            <!--日志文件保留天数-->            <MaxHistory>30</MaxHistory>            <maxFileSize>4MB</maxFileSize>            <totalSizeCap>10MB</totalSizeCap>        </rollingPolicy>        <!--设置后会过滤-->        <filter class="ch.qos.logback.classic.filter.LevelFilter">        <level>INFO</level>        <onMatch>ACCEPT</onMatch>        <onMismatch>DENY</onMismatch>        </filter>        <layout class="ch.qos.logback.classic.PatternLayout">            <!--格式化输出：%d表示日期，%thread表示线程名，%-5level：级别从左显示5个字符宽度%msg：日志消息，%n是换行符-->            <Pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} -- [%file:%line] - %msg%n</Pattern>        </layout>    </appender>    <appender name="WARN_FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">        <File>${LOG_HOME}/community-warn.log</File>        <rollingPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedRollingPolicy">            <FileNamePattern>${LOG_HOME}/community-warn.%d{yyyy-MM-dd}.%i.log</FileNamePattern>            <!--日志文件保留天数-->            <MaxHistory>30</MaxHistory>            <maxFileSize>4MB</maxFileSize>            <totalSizeCap>10MB</totalSizeCap>        </rollingPolicy>        <!--设置后会过滤-->        <filter class="ch.qos.logback.classic.filter.LevelFilter">            <level>WARN</level>            <onMatch>ACCEPT</onMatch>            <onMismatch>DENY</onMismatch>        </filter>        <layout class="ch.qos.logback.classic.PatternLayout">            <!--格式化输出：%d表示日期，%thread表示线程名，%-5level：级别从左显示5个字符宽度%msg：日志消息，%n是换行符-->            <Pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} -- [%file:%line] - %msg%n</Pattern>        </layout>    </appender>    <appender name="ERROR_FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">        <File>${LOG_HOME}/community-error.log</File>        <rollingPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedRollingPolicy">            <FileNamePattern>${LOG_HOME}/community-error.%d{yyyy-MM-dd}.%i.log</FileNamePattern>            <!--日志文件保留天数-->            <MaxHistory>30</MaxHistory>            <maxFileSize>4MB</maxFileSize>            <totalSizeCap>10MB</totalSizeCap>        </rollingPolicy>        <filter class="ch.qos.logback.classic.filter.LevelFilter">            <level>ERROR</level>            <onMatch>ACCEPT</onMatch>            <onMismatch>DENY</onMismatch>        </filter>        <layout class="ch.qos.logback.classic.PatternLayout">            <Pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} -- [%file:%line] - %msg%n</Pattern>        </layout>    </appender>    <!-- 设置Spring&Hibernate日志输出级别 -->    <logger name="org.springframework" level="WARN"/>    <logger name="org.springframework.scheduling" level="error"/>    <Logger name="org.apache.catalina.util.LifecycleBase" level="error"/>    <logger name="org.mybatis" level="error"/>    <logger name="tk.mybatis" level="error"/>    <logger name="org.hibernate.validator" level="error"/>    <logger name="io.lettuce" level="INFO" />    <logger name="org.mongodb.driver.cluster" level="OFF"/>    <logger name="org.thymeleaf" level="INFO"/>    <logger name="java.sql" level="error"/>    <logger name="java.sql.Connection" level="DEBUG"/>    <logger name="java.sql.Statement" level="DEBUG"/>    <logger name="java.sql.PreparedStatement" level="DEBUG"/>    <logger name="java.sql.ResultSet" level="error"/>    <!--<logger name="com.xfw.community.db.dao" level="DEBUG" />-->    <!-- 从高到地低 OFF 、 FATAL 、 ERROR 、 WARN 、 INFO 、 DEBUG 、 TRACE 、 ALL -->    <root level="${logback.level}">        <appender-ref ref="STDOUT"/>        <appender-ref ref="INFO_FILE"/>        <appender-ref ref="WARN_FILE"/>        <appender-ref ref="ERROR_FILE"/>        <appender-ref ref="CATALINA"/>        <!--<appender-ref ref="SYSLOG"/>-->    </root>    <!--日志异步到数据库 -->    <!--<appender name="DB" class="ch.qos.logback.classic.db.DBAppender">-->    <!--&lt;!&ndash;日志异步到数据库 &ndash;&gt;-->    <!--<connectionSource class="ch.qos.logback.core.db.DriverManagerConnectionSource">-->    <!--&lt;!&ndash;连接池 &ndash;&gt;-->    <!--<dataSource class="com.mchange.v2.c3p0.ComboPooledDataSource">-->    <!--<driverClass>com.mysql.jdbc.Driver</driverClass>-->    <!--<url>jdbc:mysql://127.0.0.1:3306/databaseName</url>-->    <!--<user>root</user>-->    <!--<password>root</password>-->    <!--</dataSource>-->    <!--</connectionSource>-->    <!--</appender>--></configuration>